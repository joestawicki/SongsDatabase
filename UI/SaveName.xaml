<Window x:Class="PianoSongs.UI.SaveName"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Print Name:" Height="250" Width="520"
        Icon="/PianoSongs;component/Icons/piano.ico"
        Style="{StaticResource BlueWindow}" MinHeight="250" MinWidth="520"
        MaxHeight="250" MaxWidth="520">
    <Grid>
        <Label Content="Print Document Title:" HorizontalAlignment="Left" Margin="28,76,0,0" VerticalAlignment="Top" Height="36" Width="206" FontSize="18" FontWeight="Bold"/>
        <TextBox HorizontalAlignment="Left" Name="textBoxTitle" Height="36" Margin="234,76,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="261" FontSize="18" VerticalContentAlignment="Center"/>
        <Button Content="OK" Margin="122,0,300,51" Name="btnOk" Style="{StaticResource GlassButtonStyle}" Height="37" VerticalAlignment="Bottom" Width="Auto" MinWidth="98" MinHeight="37" MaxWidth="98" MaxHeight="37" Click="btnOk_Click" IsDefault="True"/>
        <Button Content="Cancel" Margin="285,0,130,51" Name="btnCancel" Style="{StaticResource GlassButtonStyle}" Height="37" VerticalAlignment="Bottom" MinWidth="98" MinHeight="37" MaxWidth="98" MaxHeight="37" Click="btnCancel_Click" />


    </Grid>
</Window>
